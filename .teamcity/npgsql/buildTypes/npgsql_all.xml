<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="f07bf931-feb1-4d2a-8cfb-611559e76908" paused="true" xsi:noNamespaceSchemaLocation="http://www.jetbrains.com/teamcity/schemas/9.0/project-config.xsd">
  <name>All</name>
  <description>Triggers the different .NET and mono builds, collects all artifacts and generates nuget and docs</description>
  <settings>
    <options>
      <option name="allowExternalStatus" value="true" />
      <option name="artifactRules" value="LICENSE.txt =&gt; Npgsql-net40.zip&#xA;README.* =&gt; Npgsql-net40.zip&#xA;LICENSE.txt =&gt; Npgsql-net45.zip&#xA;README.* =&gt; Npgsql-net45.zip&#xA;LICENSE.txt =&gt; Npgsql-net40.tar.gz&#xA;README.* =&gt; Npgsql-net40.tar.gz&#xA;LICENSE.txt =&gt; Npgsql-net45.tar.gz&#xA;README.* =&gt; Npgsql-net45.tar.gz&#xA;Npgsql/bin/Release-net40/** =&gt; Npgsql-net40.zip&#xA;Npgsql.EntityFramework/bin/Release-net40/Npgsql.EntityFramework* =&gt; Npgsql-net40.zip&#xA;Npgsql.EntityFramework/bin/Legacy-Release-net40/Npgsql.EntityFramework* =&gt; Npgsql-net40.zip&#xA;Npgsql/bin/Release-net45/** =&gt; Npgsql-net45.zip&#xA;Npgsql.EntityFramework/bin/Release-net45/Npgsql.EntityFramework* =&gt; Npgsql-net45.zip&#xA;Npgsql.EntityFramework/bin/Legacy-Release-net45/Npgsql.EntityFramework* =&gt; Npgsql-net45.zip&#xA;Npgsql/bin/Release-net40/** =&gt; Npgsql-net40.tar.gz&#xA;Npgsql.EntityFramework/bin/Release-net40/Npgsql.EntityFramework* =&gt; Npgsql-net40.tar.gz&#xA;Npgsql.EntityFramework/bin/Legacy-Release-net40/Npgsql.EntityFramework* =&gt; Npgsql-net40.tar.gz&#xA;Npgsql/bin/Release-net45/** =&gt; Npgsql-net45.tar.gz&#xA;Npgsql.EntityFramework/bin/Release-net45/Npgsql.EntityFramework* =&gt; Npgsql-net45.tar.gz&#xA;Npgsql.EntityFramework/bin/Legacy-Release-net45/Npgsql.EntityFramework* =&gt; Npgsql-net45.tar.gz&#xA;NpgsqlDdexProvider/bin/Release/NpgsqlDdexProvider* =&gt; NpgsqlDdexProvider-VS2010.zip&#xA;NpgsqlDdexProvider/bin/Release-net45/NpgsqlDdexProvider* =&gt; NpgsqlDdexProvider-VS2013.zip&#xA;docs/api/** =&gt; docs.zip" />
    </options>
    <parameters />
    <build-runners>
      <runner id="RUNNER_6" name="" type="jb.nuget.pack">
        <parameters>
          <param name="nuget.pack.as.artifact" value="true" />
          <param name="nuget.pack.include.sources" value="true" />
          <param name="nuget.pack.output.clean" value="true" />
          <param name="nuget.pack.output.directory" value="packages" />
          <param name="nuget.pack.project.dir" value="as_is" />
          <param name="nuget.pack.properties" value="Configuration=Release" />
          <param name="nuget.pack.specFile"><![CDATA[Npgsql\Npgsql.nuspec
Npgsql.EntityFramework/Npgsql.EntityFramework.nuspec
Npgsql.EntityFramework/Npgsql.EntityFrameworkLegacy.nuspec]]></param>
          <param name="nuget.path" value="?NuGet.CommandLine.DEFAULT.nupkg" />
          <param name="nugetCustomPath" value="?NuGet.CommandLine.DEFAULT.nupkg" />
          <param name="nugetPathSelector" value="?NuGet.CommandLine.DEFAULT.nupkg" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
      <runner id="RUNNER_3" name="Generate documentation" type="simpleRunner">
        <parameters>
          <param name="command.executable" value="c:\dev\doxygen\bin\doxygen.exe" />
          <param name="command.parameters" value="Npgsql.doxyfile" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="npgsql_github" />
    </vcs-settings>
    <requirements />
    <build-triggers>
      <build-trigger id="vcsTrigger" type="vcsTrigger">
        <parameters>
          <param name="branchFilter" value="+:refs/heads/*" />
          <param name="quietPeriodMode" value="DO_NOT_USE" />
        </parameters>
      </build-trigger>
    </build-triggers>
    <build-extensions>
      <extension id="BUILD_EXT_1" type="teamcity.github.status">
        <parameters>
          <param name="guthub_host" value="https://api.github.com" />
          <param name="guthub_owner" value="npgsql" />
          <param name="guthub_repo" value="Npgsql" />
          <param name="guthub_username" value="npgsql-teamcity" />
          <param name="secure:guthub_username" value="zxx9b1a45147d7e64e7775d03cbe80d301b" />
        </parameters>
      </extension>
    </build-extensions>
    <artifact-dependencies>
      <dependency sourceBuildTypeId="npgsql_net40" cleanDestination="false">
        <revisionRule name="sameChainOrLastFinished" revision="latest.sameChainOrLastFinished" />
        <artifact sourcePath="**" />
      </dependency>
      <dependency sourceBuildTypeId="npgsql_net45" cleanDestination="false">
        <revisionRule name="sameChainOrLastFinished" revision="latest.sameChainOrLastFinished" />
        <artifact sourcePath="**" />
      </dependency>
    </artifact-dependencies>
    <dependencies>
      <depend-on sourceBuildTypeId="npgsql_net40">
        <options>
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
      <depend-on sourceBuildTypeId="npgsql_net45">
        <options>
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
      <depend-on sourceBuildTypeId="npgsql_mono45">
        <options>
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
    </dependencies>
    <cleanup />
  </settings>
</build-type>

