  "repository": {
    "type": "git",
    "url": "git://github.com/npgsql/npgsql"
  },
  "compile": "../Shared/*.cs",
  "resourceFiles": "NpgsqlMetaData.xml",
  "compilationOptions": {
    "warningsAsErrors": true,
    "define": [ "__MonoCS__" ],
    "keyFile": "../../Npgsql.snk"
  },
  "commands": {
    "rewrite-async": "AsyncRewriter"
  },
  "scripts": {
    "prebuild": [
      "dnx rewrite-async"
    ]
  },
